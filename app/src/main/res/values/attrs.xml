<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableViewWidget">
        <attr name="title_text" format="string" />
        <attr name="description_text" format="string" />
        <attr name="expanded_view" format="string" />
        <attr name="chevron_src" format="reference" />
        <attr name="paddingTop" format="dimension" />
        <attr name="paddingBottom" format="dimension" />
        <attr name="paddingStart" format="dimension" />
        <attr name="paddingEnd" format="dimension" />
        <attr name="titleTextSize" format="dimension" />
        <attr name="descriptionTextSize" format="dimension" />
        <attr name="descriptionMaxLines" format="integer" />
        <attr name="dividerLineVisibility">
            <flag name="invisible" value="0x00000004" />
            <flag name="visible" value="0x00000000" />
        </attr>
    </declare-styleable>
</resources>